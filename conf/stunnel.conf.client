foreground = yes
debug = 5
output = /dev/stdout
syslog = no

cert = /etc/stunnel/node_client_cert.pem
key = /etc/stunnel/node_client_key.pem
verifyChain = yes
CAfile = /etc/stunnel/ca_public_cert.pem
options = NO_SSLv2

[tarantool]
client = yes
accept = 127.0.0.1:3313
connect = %POSTANALYTIC_ADDRESS%
